        <property name="styleSheet">
         <string notr="true">/* Main calendar widget style */
QCalendarWidget {
    border: 1px solid #b0b0b0;
    border-radius: 8px;
    background-color: #fdfdfd;
    font-family: Serifa BT;
    font-size: 13px;
    color: #333333;
}

/* Header (Month/Year navigation) */
QCalendarWidget QToolButton {
    background-color: #f0f0f0;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
    color: #333333;
    margin: 2px;
    padding: 3px 8px;
}

/* Hover effect on navigation buttons */
QCalendarWidget QToolButton:hover {
    background-color: #e6e6e6;
}

/* Pressed effect on navigation buttons */
QCalendarWidget QToolButton:pressed {
    background-color: #dcdcdc;
}

/* Navigation arrows for Month/Year */
QCalendarWidget QToolButton#qt_calendar_prevmonth, 
QCalendarWidget QToolButton#qt_calendar_nextmonth {
    width: 30px; /* Button width */
    height: 30px; /* Button height */
    border-radius: 4px;
    background-color: transparent;
    border: none;
}

/* Hover effect for navigation arrows */
QCalendarWidget QToolButton#qt_calendar_prevmonth:hover,
QCalendarWidget QToolButton#qt_calendar_nextmonth:hover {
    background-color: #d0eaff; /* Light blue background on hover */
    border-radius: 4px;
}

/* Dropdown for month selector */
QCalendarWidget QToolButton::menu-indicator {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    padding-right: 5px;
}

/* Month selector menu */
QCalendarWidget QMenu {
    background-color: #ffffff;
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    color: #333333;
    padding: 4px;
}

QCalendarWidget QMenu::item {
    padding: 5px 10px;
    background-color: #ffffff;
    color: #333333;
}

QCalendarWidget QMenu::item:selected {
    background-color: #4caf50;
    color: #ffffff;
}

/* Weekday headers */
QCalendarWidget QHeaderView {
    background-color: #f0f0f0;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
}

QCalendarWidget QHeaderView::section {
    background-color: #e9e9e9;
    color: #555555;
    padding: 5px;
    border: none;
}

/* Days of the month */
QCalendarWidget QTableView {
    border: 1px solid #d0d0d0;
    gridline-color: #e0e0e0;
    background-color: #ffffff;
}

QCalendarWidget QTableView::item {
    color: #333333;
    border: none;
    padding: 5px;
}

QCalendarWidget QTableView::item:hover {
    background-color: #f5f5f5;
    border-radius: 4px;
}

QCalendarWidget QTableView::item:selected {
    background-color: #4caf50;
    color: white;
    border-radius: 4px;
}

/* Today highlight */
QCalendarWidget QWidget#qt_calendar_today {
    border: 1px solid #ff5722;
    background-color: #ffeeee;
    color: #ff5722;
    border-radius: 3px;
    margin: 1px;
    padding: 3px;
}

/* Navigation bar */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #f0f0f0;
    border-bottom: 1px solid #d0d0d0;
    border-radius: 4px;
}
</string>
